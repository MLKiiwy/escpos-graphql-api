{
  "name": "escpos-printer-api",
  "version": "0.0.0-development",
  "author": "Mikael Labrut",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "analyze": "source-map-explorer build/static/js/main.*",
    "start:dev:client": "webpack-dev-server --config ./config/webpack.config.js",
    "start:dev:server": "nodemon src/server.js --exec babel-node",
    "start:server": "node dist/server.js",
    "prebuild": "rm -rf dist",
    "build:client": "webpack --config ./config/webpack.production.config.js",
    "build:server": "babel src/server.js --out-file ./dist/server.js",
    "test": "jest --config ./config/jest.config.json --color --runInBand",
    "test:watch": "jest --config ./config/jest.config.json --watch",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commitmsg": "commitlint --config ./config/commitlint.config.js -E GIT_PARAMS",
    "ticket:generate": "node screenshot.js",
    "ticket:print": "node print.js",
    "db:migrate:up": "knex migrate:latest",
    "db:migrate:down": "knex migrate:rollback",
    "db:migrate:make": "knex migrate:make migration",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "config": "^2.0.1",
    "cors": "^2.8.5",
    "escpos": "^2.4.9",
    "express": "^4.16.4",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.9.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "showdown": "^1.8.7",
    "sqlite3": "^4.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@storybook/react": "^4.0.4",
    "autoprefixer": "^9.0.2",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "jest-cucumber": "^1.0.11",
    "jest-junit": "^5.1.0",
    "js-yaml": "^3.12.0",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "nock": "^10.0.2",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.6",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.13.5",
    "sass-loader": "^7.0.3",
    "source-map-explorer": "^1.5.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-links": "^4.0.4",
    "@storybook/addons": "^4.0.4"
  }
}
